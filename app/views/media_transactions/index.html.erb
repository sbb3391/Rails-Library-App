<h1>Browse All Your Transactions:</h1>

<div>
<% @media_transactions.order(:created_at).each do |mt| %>
  <div class="my-8">
    <h1 class="font-bold"><%= mt.created_at.strftime("%m-%d-%Y") %></h1>
    <% mt.books.each do |book| %>
      <h4><%= book.title %></h4>
    <% end %>
  </div>
<% end %>
</div>